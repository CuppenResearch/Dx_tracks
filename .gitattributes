*.list filter=lfs diff=lfs merge=lfs -text
*.bed filter=lfs diff=lfs merge=lfs -text
*.vcf filter=lfs diff=lfs merge=lfs -text
*.tsv filter=lfs diff=lfs merge=lfs -text
*.interval_list filter=lfs diff=lfs merge=lfs -text
*.UD filter=lfs diff=lfs merge=lfs -text
*.refflat filter=lfs diff=lfs merge=lfs -text

# Disable git-lfs for pharmacogenetics bed and vcf
pharmacogenetics/*.bed -filter=lfs -diff=lfs -merge=lfs -text
pharmacogenetics/*.vcf -filter=lfs -diff=lfs -merge=lfs -text
